{
  "schemaVersion": 1,
  "id": "carpet-redcraft-addons",
  "version": "${version}",

  "name": "Carpet RedCraft Addons",
  "description": "Addons for the RedCraft server",
  "authors": [
    "BisUmTo",
    "replaceitem",
    "Gnottero",
    "DukeEdivad05",
    "RubixDev",
    "..."
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/carpet-redcraft-addons",
    "issues": "https://github.com/MultiCoreNetwork/carpet-redcraft-addons/issues",
    "sources": "https://github.com/MultiCoreNetwork/carpet-redcraft-addons"
  },
  "license": "GNU-3.0",
  "icon": "assets/carpet-redcraft-addons/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
        "it.multicoredev.carpetredcraftaddons.CarpetRedCraftExtension"
    ]
